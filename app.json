{
  "expo": {
    "name": "Nojsys Ops",
    "slug": "nojsysops",
    "version": "STABLE_1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_bbic.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_bbic.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bbic.nojsysapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_bbic.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "af9c65e9-bef1-4587-8fe4-469beb62ca22"
      }
    }
  }
}
